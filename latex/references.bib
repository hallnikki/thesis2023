@article{moreno_introducing_2021,
	title = {Introducing the “15-Minute City”: Sustainability, Resilience and Place Identity in Future Post-Pandemic Cities},
	volume = {4},
	rights = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2624-6511},
