@article{giles-corti_increasing_2005,
	title = {Increasing walking: How important is distance to, attractiveness, and size of public open space?},
	volume = {28},
	issn = {0749-3797},
	url = {https://www.sciencedirect.com/science/article/pii/S0749379704002983},
	doi = {10.1016/j.amepre.2004.10.018},
	series = {Active Living Research},
	shorttitle = {Increasing walking},
	abstract = {Background
Well-designed public open space ({POS}) that encourages physical activity is a community asset that could potentially contribute to the health of local residents.
Methods
In 1995–1996, two studies were conducted—an environmental audit of {POS} over 2 acres (n =516) within a 408-km2 area of metropolitan Perth, Western Australia; and personal interviews with 1803 adults (aged 18 to 59 years) (52.9\% response rate). The association between access to {POS} and physical activity was examined using three accessibility models that progressively adjusted for distance to {POS}, and its attractiveness and size. In 2002, an observational study examined the influence of attractiveness on the use of {POS} by observing users of three pairs of high- and low-quality (based on attractiveness) {POS} matched for size and location.
Results
Overall, 28.8\% of respondents reported using {POS} for physical activity. The likelihood of using {POS} increased with increasing levels of access, but the effect was greater in the model that adjusted for distance, attractiveness, and size. After adjustment, those with very good access to large, attractive {POS} were 50\% more likely to achieve high levels of walking (odds ratio, 1.50; 95\% confidence level, 1.06–2.13). The observational study showed that after matching {POS} for size and location, 70\% of {POS} users observed visited attractive {POS}.
Conclusions
Access to attractive, large {POS} is associated with higher levels of walking. To increase walking, thoughtful design (and redesign) of {POS} is required that creates large, attractive {POS} with facilities that encourage active use by multiple users (e.g., walkers, sports participants, picnickers).},
	pages = {169--176},
	number = {2},
	journaltitle = {American Journal of Preventive Medicine},
	shortjournal = {American Journal of Preventive Medicine},
	author = {Giles-Corti, Billie and Broomhall, Melissa H. and Knuiman, Matthew and Collins, Catherine and Douglas, Kate and Ng, Kevin and Lange, Andrea and Donovan, Robert J.},
	urldate = {2023-06-24},
	date = {2005-02-01},
	langid = {english},
}

@article{sundara_rajoo_addressing_2021,
	title = {Addressing psychosocial issues caused by the {COVID}-19 lockdown: Can urban greeneries help?},
	volume = {65},
	issn = {1618-8667},
	url = {https://www.sciencedirect.com/science/article/pii/S1618866721003678},
	doi = {10.1016/j.ufug.2021.127340},
	shorttitle = {Addressing psychosocial issues caused by the {COVID}-19 lockdown},
	abstract = {The Coronavirus disease-2019 ({COVID}-19) pandemic has affected over 200 countries, forcing governments to impose lockdowns to contain the spread of the disease. Although effective in reducing infection rates, the lockdowns have also resulted in a severe negative impact on mental health throughout the world; Setting the foundation for mental illnesses to become the next “silent” pandemic. This study attempts to determine a self-care method of ensuring mental health during the {COVID}-19 pandemic, especially for those living under lockdown. We evaluated the potential of physical exercise (in a nature setting) and nature therapy in improving mental wellbeing, among young adults with either stress, anxiety or depression symptoms. The study involved thirty subjects, who were equally divided into a nature-exercise group and a nature therapy group. The participants were briefed on the activities that they were to perform on a daily basis, and both groups performed their assigned activities concurrently for one week (27th April 2020 to 3rd May 2020) at urban greeneries accessible to them (rooftop parks, neighbourhood parks, home gardens). We used the depression, anxiety and stress scale – 21 items ({DASS}-21) to evaluate the mental health status of participants, once before beginning the study (baseline readings) and once at the end of the study (after a week of nature-exercise/nature therapy). There was a statistically significant reduction in stress, anxiety and depression symptoms for both the nature-exercise and nature therapy groups. However, when evaluating the effectiveness of exercise and nature therapy in treating stress, anxiety and depression symptoms on a case-by-case basis, it was discovered that nature therapy was more effective in treating mental health issues. Hence, nature therapy has the potential to be a form of preventive medicine, namely in preserving mental health during the {COVID}-19 crisis.},
	pages = {127340},
	journaltitle = {Urban Forestry \& Urban Greening},
	shortjournal = {Urban Forestry \& Urban Greening},
	author = {Sundara Rajoo, Keeren and Singh Karam, Daljit and Abdu, Arifin and Rosli, Zamri and James Gerusu, Geoffery},
	urldate = {2023-06-23},
	date = {2021-11-01},
	langid = {english},
	keywords = {Anxiety, {COVID}-19, {DASS}-21, Depression, Exercise, Nature therapy, Preventive medicine, Stress},
}

